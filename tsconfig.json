{
  "compilerOptions": {
    "outDir": "./www/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "strictNullChecks": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es6"
    ]
  },
  "include": [
    "src/**/*"
  ]
}
